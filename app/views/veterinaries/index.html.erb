 
<div class="row">
  <div class="col-lg-12">
    <div class="card">
      <div class="header">
        <h4 class="title"><i class="pe-7s-eyedropper"></i> Veterinarios</h4>
        <p class="category"><%= notice %></p>
      </div> 
      <div class="content">  
       
        <%= link_to  new_veterinary_path , {remote:true, class: "fcbtn btn btn-success btn-1e" } do %>
        <i class="icon-plus"></i>
        <span>Nuevo</span>
        <%end%>

        <%= link_to  filter_veterinaries_path , {remote:true, class: "fcbtn btn btn-primary btn-1e" } do %>
        <i class="icon-search"></i>
        <span>Buscar</span>
        <%end%>

        
        <table class="table">
          <thead>
            <tr>
              <th> <%= t('veterinaries.id')%> </th>
              <th><%= t('veterinaries.name')%></th>
              <th><%= t('veterinaries.email')%></th>
              <th><%= t('veterinaries.created_at')%></th> 
              <th colspan="3">Acciones</th>
            </tr> 
          </thead>
          <tbody class="veterinaries-table-index">
            <%= render 'veterinaries/partials/index'%>
          </tbody>
          <tfoot>
            <td colspan="7">
              <div class="text-right">
                <ul class="pagination">
                  <%= paginate @veterinaries %>
                </ul>
              </div>
            </td>
          </tfoot>
        </table>  
      </div>
    </div>
  </div>
</div> 

<div id="modal-veterinaries" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
</div>