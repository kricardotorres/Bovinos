$("ul.errors").html("")
<% if @cow.errors.any? %>
  <% @cow.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %> 
  $("#modal-cows").animate({ scrollTop: 0 }, 'slow')
<% else %>
  $(".cows-table-index").html("<%= j(render 'cows/partials/index') %>")
  $("ul.msg").append($("<li />").html("<%= @msgResult %>"))
  $("#modal-cows").modal("hide")
<% end %>