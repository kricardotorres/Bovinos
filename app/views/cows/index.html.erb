 
<div class="row">
<div class="col-md-12"> 
                        <div class="card">
        <div class="header">
            <h4 class="title"><i class="pe-7s-medal"></i> Bovinos</h4>
            <p class="category"><%= notice %></p>
        </div>                   
      <div class="content">
      <%= link_to  new_cow_path , {remote:true, class: "fcbtn btn btn-success btn-1e" } do %>
      <i class="icon-plus"></i>
      <span>Nuevo</span>
      <%end%>

       <%= link_to  filter_cows_path , {remote:true, class: "fcbtn btn btn-primary btn-1e" } do %>
      <i class="icon-search"></i>
      <span>Buscar</span>
      <%end%>

     
        <table class="table">
          <thead>
            <tr>
              <th> <%= t('cows.id')%> </th>
              <th><%= t('cows.name')%></th> 
              <th><%= t('cows.created_at')%></th> 
              <th colspan="3">Acciones</th>
            </tr> 
          </thead>
          <tbody class="cows-table-index">
            <%= render 'cows/partials/index'%>
          </tbody>
          <tfoot>
            <td colspan="7">
              <div class="text-right">
                <ul class="pagination">
                  <%= paginate @cows %>
                </ul>
              </div>
            </td>
          </tfoot>
        </table>  

                                <div class="footer">
                                 
                                    <div class="stats">
                                       
                                    </div>
                                </div>
      </div>
    </div>
  </div>
</div> 

<div id="modal-cows" class="modal fade " tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
</div>